program repeat_until_tersarang;
uses wincrt;
var
   a,b,c:real;
begin
     writeln('--------------------');
     writeln('sisi A  sisi B  sisi C');
     writeln('--------------------');
     a:=1;
     repeat
           b:=0;
           repeat
                 c:=sqrt(a*a+b*b);
                 writeln(a:6:2,b:9:2,c:9:2);
                 b:=b+5;
           until b>25;
           a:=a+1;
     until a>3;
     writeln('---------------------');
end.